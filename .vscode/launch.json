{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Dev Android (flavor dev)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "debug",
      "deviceId": "emulator-5554",
      "program": "lib/main.dart",
      "args": [
        "--flavor",
        "dev"
      ]
    },
    {
      "name": "Dev iOS (flavor dev)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "debug",
      "deviceId": "ios",
      "program": "lib/main.dart",
      "args": [
        "--flavor",
        "dev"
      ]
    },
    {
      "name": "Dev Windows (no flavor)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "debug",
      "deviceId": "windows",
      "program": "lib/main.dart"
    },

    {
      "name": "C/C++ Runner: Debug Session",
      "type": "cppdbg",
      "request": "launch",
      "args": [],
      "stopAtEntry": false,
      "externalConsole": true,
      "cwd": "${workspaceFolder}",
      "program": "${workspaceFolder}/build/Debug/outDebug",
      "MIMode": "gdb",
      "miDebuggerPath": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
  ]
}
